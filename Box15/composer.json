{
    "name": "your-vendor/callback-registry",
    "description": "A robust solution for registering and managing content generation callbacks in PHP background jobs",
    "type": "library",
    "keywords": ["callback", "registry", "background-jobs", "serialization", "isr", "cache"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "classmap": [
            "CallbackRegistry.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "CallbackRegistryTest.php"
        ]
    },
    "scripts": {
        "test": "phpunit CallbackRegistryTest.php",
        "demo": "php demo.php",
        "demo:all": "php demo.php --all",
        "demo:basic": "php demo.php 1",
        "demo:jobs": "php demo.php 2",
        "demo:wordpress": "php demo.php 3",
        "demo:laravel": "php demo.php 4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
